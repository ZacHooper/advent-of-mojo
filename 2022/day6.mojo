from algorithm import sort

fn main():
    let test_input: String = "mjqjpqmgbljsphdztnvjfqwrcgsmlb"
    let puzzle_input: String = ""
    let input = test_input
    let window_size = 16
    let buffer: SIMD[DType.uint16, 16]
    for i in  range(len(input)):
        buffer = SIMD[DType.uint16, 16](ord(input[i]), ord(input[i+1]), ord(input[i+2]), ord(input[i+3]), ord(input[i+4]), ord(input[i+5]), ord(input[i+6]), ord(input[i+7]), ord(input[i+8]), ord(input[i+9]), ord(input[i+10]), ord(input[i+11]), ord(input[i+12]), ord(input[i+13]), 1, 2)

        # See if any characters are duplicated in the buffer
        let shifted_1 = buffer.rotate_right[1]()
        let shifted_2 = buffer.rotate_right[2]()
        # Check for matches. If there is no duplicate then the result will be True
        let duplicates = (buffer == shifted_1) | (buffer == shifted_2)
        let has_duplicate = duplicates.reduce_or().__invert__()
        # Invert so has dupe is True
        if has_duplicate:
            print("Found duplicate in buffer: ", buffer, " at index: ", i + window_size)
            break



